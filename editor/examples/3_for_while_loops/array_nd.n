z = [ 1 , 2 , 4, 5, 10 ]
x = [ 5-6, 8-10, 8*4 - 4*9, -5, 9 -11 - 8 ]

i = 0
@( i < len(x) )  வரை
   பதிப்பி    z[i] +  x[i]
   assert( ( z[i] +  x[i] ) == 0 )
   i = i + 1
